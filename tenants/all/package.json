{
  "name": "@pmmi-media-group/all",
  "version": "2.4.1",
  "author": "Brandon Krigbaum <brandon@parameter1.com>",
  "repository": "https://github.com/parameter1/pmmi-media-group-newsletters/tree/master/tenants/all",
  "license": "MIT",
  "private": true,
  "newsletter": {
    "account": "pmmi",
    "tenant": "all",
    "stack": "tauron",
    "deploy": true
  },
  "scripts": {
    "build": "yarn compile",
    "dev": "mindful-web-newsletters dev --compile-dir ../../packages --watch-dir ../../packages",
    "compile": "mindful-web-marko-compile compile",
    "lint": "eslint --ext .js --max-warnings 5 --config ../../.eslintrc.js --ignore-path ../../.eslintignore ./",
    "lint:fix": "yarn lint --fix",
    "test": "yarn lint && yarn compile --no-clean"
  },
  "engines": {
    "node": ">=14.15"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "dependencies": {
    "@mindful-web/marko-core": "^1.28.0",
    "@mindful-web/marko-newsletters": "^1.28.0",
    "@mindful-web/marko-newsletters-email-x": "^1.45.4",
    "@mindful-web/marko-newsletters-native-x": "^1.45.1",
    "@mindful-web/newsletter-cli": "^1.11.9",
    "@mindful-web/object-path": "^1.11.9",
    "@pmmi-media-group/package-common": "^2.4.1",
    "graphql": "^14.5.4",
    "graphql-tag": "^2.10.1",
    "newrelic": "^9.10.1"
  }
}
